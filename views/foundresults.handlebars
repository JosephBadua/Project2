<div class="container-fluid top">
  <div class="row">
    <div class="col-md-2">
      <nav class="navbar navbar-expand-lg navbar-dark">
        <a class="navbar-brand" href="/">Dude, Wheres My Dog?</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02">
          <span class="navbar-toggler-icon"></span>
        </button>
      </nav>
    </div>
    <div class="col-md-2 offset-md-1">
      <a href="/WhatWeDo">
        <h1 class="topnav">What We Do!</h1>
      </a>
    </div>
    <div class="col-md-2">
      <a href="/Report">
        <h1 class="topnav">Report a Lost Animal</h1>
      </a>
    </div>
    <div class="col-md-2">
      <a href="/Rescued">
        <h1 class="topnav">Our Results!</h1>
      </a>
    </div>
    <div class="col-md-1 offset-md-1">
      <a class="nav-link link" href="">Contact</a>
    </div>
    <div class="col-md-1">
      <a class="nav-link link" href=""> Sign In</a>
    </div>
  </div>
</div>

<div class="container-fluid">
  <div class="row">
    <div class="col-md-6">
      <h1>Hello</h1>
    </div>
    <div class="col-md-6 text-center main">
      <ul>
        <div class="col-md-12">
          <h1 align="center">Found Pet Results</h1>
        </div>
        {{#each animals}}
        <div class="petContainer">
          <div class="row">
            <div class="col-md-1">
              <h6 class="petName">Name:</h6>
            </div>
            <div class="col-md-3">
              <h2> {{this.animal_name}}</h2>
              <hr>
              <img src="{{this.picture}}" alt="animal profile picture" class="petPic zoom"> </img>
            </div>
            <div class="col-md-8">
              <li class="listTop"> <b>Species:</b> {{this.species}}</li>
              <li> <b>Breed:</b> {{this.breed}}</li>
              <li> <b>Color:</b> {{this.color}}</li>
              <li> <b>Description:</b> {{this.animal_description}}</li>
              <li> <b>Lost Date:</b> {{this.lost_date}}</li>
              <li> <b>Where Lost:</b> {{this.lost_where}}</li>
              <li> <b>Zip Code:</b> {{this.zip_code}}</li>
              <li> <b>Contact Email:</b> {{this.contact_email}}</li>
            </div>
            <hr>
          </div>
        </div>
        <br>
        {{/each}}
      </ul>
    </div>
  </div>
</div>

<div class="container-fluid bottom">
  <div class="row">
    <div class="col-md-6 text-right">
      <footer id="footer">
        <p>Â© Copyright 2019 Dude, Wheres My Dog?</p>
      </footer>
    </div>
    <div class="col-md-6 text-left">
      <a href="https://github.com/mjjohnston55/Project2" id="repoLink">Github Repository</a>
    </div>
  </div>
</div>